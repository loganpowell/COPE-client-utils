{
    "name": "cope-client-utils",
    "author": "Logan Powell",
    "license": "MIT",
    "version": "0.1.7",
    "description": "CRUD utilities and types for the COPE platform",
    "type": "module",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "repository": "https://github.com/loganpowell/COPE-client-utils",
    "homepage": "https://github.com/loganpowell/COPE-client-utils",
    "scripts": {
        "pages": "npm run types && npm run jsdoc",
        "clean": "rm -rf ./lib & rm -rf ./dist &  rm -rf .cache",
        "madge": "madge --circular lib/",
        "typewatch": "tsc --project tsconfig.json",
        "types": "tsc --project tsconfig.build.json",
        "jsdoc": "jsdoc -c jsdoc/jsdoc.json --verbose --debug",
        "docs": "npm run types && npm run jsdoc",
        "patch": "npm version patch && npm run docs && npm run pages && npm publish",
        "test": "npm run madge && concurrently \"npm run typewatch\" \"jest --watchAll\"",
        "postgit": "git push origin master && npm run patch",
        "ncu": "ncu -u && npm i && npm audit fix",
        "git": "npm run ncu && git add . && git commit -m",
        "build": "npm run clean && cross-env NODE_ENV=production parcel build client/index.html -d dist --no-source-maps --no-cache --public-url ./",
        "dev": "concurrently \"npm run typewatch\" \"parcel client/index.html --global md -p 8080 --open -d dist watch\""
    },
    "keywords": [],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@-0/browser": "^0.3.6",
        "@aws-amplify/api": "^4.0.5",
        "@aws-amplify/auth": "^4.1.1",
        "@aws-amplify/core": "^4.1.3",
        "@aws-amplify/datastore": "^3.2.0",
        "@aws-amplify/storage": "^4.3.0",
        "@thi.ng/associative": "^5.2.6",
        "@thi.ng/checks": "^2.9.8",
        "json-to-graphql-query": "^2.1.0",
        "nanographql": "^2.0.0",
        "node-fetch": "^2.6.1",
        "uuid": "^8.3.2"
    },
    "peerDependencies": {},
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.14.5",
        "@types/jest": "^26.0.24",
        "@types/node": "^16.3.0",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "babel-jest": "^27.0.6",
        "better-docs": "^2.3.2",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "dummy-text-generator": "^1.0.3",
        "eslint": "^7.30.0",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-prettier": "^3.4.0",
        "gh-pages": "^3.2.3",
        "jest": "^27.0.6",
        "jest-environment-jsdom": "^27.0.6",
        "jsdom": "^16.6.0",
        "npm-delay": "^1.0.4",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.1.0",
        "typescript": "^4.3.5"
    },
    "browserslist": [
        "last 3 Chrome versions"
    ],
    "prettier": {
        "singleQuote": false,
        "printWidth": 100,
        "endOfLine": "lf",
        "tabWidth": 4,
        "semi": false,
        "arrowParens": "avoid"
    }
}
